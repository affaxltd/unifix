{
	"name": "UniFix",
	"description": "UniFix patches Uniswap's exchange site to remove the blocklisted tokens. I'll keep it updated for some time.",
	"version": "1.0",
	"manifest_version": 2,
	"key": "",
	"permissions": ["<all_urls>", "webRequest", "webRequestBlocking"],
	"browser_action": {
		"default_icon": "logo/Logo.png"
	},
	"icons": {
		"512": "logo/Logo.png"
	},
	"background": {
		"scripts": ["backend/bg.js"],
		"persistent": true
	},
	"web_accessible_resources": ["backend/main.js"]
}
